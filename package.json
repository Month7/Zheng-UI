{
  "name": "zheng-ui",
  "version": "0.1.8",
  "private": false,
  "main": "./dist/zheng-ui.common.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js",
    "*.css"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-z": "vue-cli-service build --target lib --name zheng-ui ./src/components/index.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "start": "vuepress dev docs"
  },
  "dependencies": {
